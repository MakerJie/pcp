        text-align: center;
        font-weight: 700;
        font-size: 20px;
        vertical-align: middle;
        padding: 12px;
    }
    .block2{
        background: url(<%= global.config.publicPath %>/page/images/icon/block2_bg_border.png) #ffffff;
        background-size: 100% 100%;
        height: 40px;
        line-height: 40px;
        text-align: center;
        color: #014288;
        font-weight: 700;
        font-size: 20px;
    }

    .block4{
        background: url(<%= global.config.publicPath %>/page/images/icon/block4_bg_border.png) #ffffff;
        background-size: 100% 100%;
