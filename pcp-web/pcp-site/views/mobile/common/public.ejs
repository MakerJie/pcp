        background: url(<%= global.config.publicPath %>/page/images/icon/box_border_top.png) #ffffff;
        background-size: 100%;
        background-repeat: no-repeat;
        height: 50px;
        text-align: center;
        position: relative;
    }
    .rights .center_img{
        background: url(<%= global.config.publicPath %>/page/images/icon/box_bg.png);
        background-repeat: repeat-y;
        background-size: 100%;
        padding: 15px;
