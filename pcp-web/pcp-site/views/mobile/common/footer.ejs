<script src="<%= global.config.publicPath %>/page/scripts/common/config.js"></script>
<script src="<%= global.config.publicPath %>/page/scripts/common/common.js"></script>

<script>
    (function () {
        org.breezee.context = {
            contextPath: "<%= global.config.contextPath %>",
            viewPrefix: "<%= global.config.viewPrefix %>",
            publicPath:"<%= global.config.publicPath %>",
            queryData: JSON.parse('<%= JSON.stringify(queryData); %>'.replace(/&#34;/g, '"') || "{}"),
            userData: JSON.parse('<%= session && JSON.stringify(session.userData); %>'.replace(/&#34;/g, '"') || "{}"),
            getImgSrc: function () {
                return "<%= global.config.imagePath %>" + (arguments[0] || 'default.png');
            }
        };
        Dolphin.path.contextPath = org.breezee.context.contextPath;
        Dolphin.defaults.url.viewPrefix = org.breezee.context.viewPrefix;
        Dolphin.defaults.img.build = function (param) {
            return org.breezee.context.getImgSrc(param.id);
        };
        Dolphin.defaults.ajax.requestHeader['mySessionCode'] = "<%= session.userData && session.userData.userCode %>";
        Dolphin.initLoadingPanel($('#progreessPanel'));
    })();
</script>
<script src="<%= global.config.publicPath %>/page/scripts/<%= path %>.js?_token=<%= new Date().getTime() %>"></script>

<script>
    $(function () {
        if (org.breezee.page && org.breezee.page.init) {
            org.breezee.page.init();
        }
        org.breezee.menu('<%= path %>');
        org.breezee.utils.navUtil();
    });
</script>

